{
  "name": "medgpt-chat",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write ./src",
    "i18n": "npm run i18n-extract && npm run i18n-compile && npm run i18n-trans",
    "i18n-extract": "formatjs extract  'src/**/*.ts*' --ignore='**/*.d.ts' --out-file lang/zh-CN.json",
    "i18n-compile": "formatjs compile \"{node_modules/**/lang,lang}/zh-CN.json\" --out-file lang/zh-CN-compiled.json",
    "i18n-trans": "i18n-trans --from lang/zh-CN-compiled.json --target zh-CN en ar th",
    "ts-check": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "@ant-design/icons": "^5.1.4",
    "@ant-design/pro-components": "^2.6.8",
    "@types/lodash.debounce": "^4.0.7",
    "@types/qs": "^6.9.7",
    "ahooks": "^3.7.8",
    "animate.css": "^4.1.1",
    "antd": "^5.7.3",
    "antd-mobile": "^5.32.4",
    "axios": "^0.19.2",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.9",
    "highlight.js": "^11.8.0",
    "immer": "^10.0.2",
    "lodash.debounce": "^4.0.8",
    "lodash.omit": "^4.5.0",
    "lodash.unionby": "^4.8.0",
    "markdown-it": "^13.0.1",
    "moment": "^2.29.4",
    "nprogress": "^0.2.0",
    "qiniu-js": "^3.4.1",
    "qs": "^6.11.2",
    "rc-virtual-list": "^3.10.5",
    "react": "18.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.0.10",
    "react-fast-marquee": "^1.6.0",
    "react-intl": "^6.4.4",
    "react-json-view": "^1.21.3",
    "react-router-dom": "^6.14.2",
    "react-toastify": "^9.1.3",
    "stylus": "^0.59.0",
    "swiper": "^9.4.1",
    "swr": "^2.2.0",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@emotion/react": "^11.11.1",
    "@formatjs/cli": "^6.1.3",
    "@types/lodash.omit": "^4.5.7",
    "@types/lodash.unionby": "^4.8.7",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.2.16",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "10.4.13",
    "babel-plugin-formatjs": "^10.5.3",
    "less": "^4.1.3",
    "postcss": "8.4.21",
    "prettier": "^2.8.8",
    "tailwindcss": "3.3.3",
    "typescript": "4.9.5",
    "vite": "^4.4.7"
  }
}
